{
  "name": "magic-wrapper-sdk",
  "version": "1.0.2",
  "description": "Internal wrapper SDK around magic-sdk for React projects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@magic-ext/oauth": "^23.0.3",
    "ethers": "^6.15.0",
    "js-cookie": "^3.0.5",
    "magic-sdk": "^29.0.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.0.0",
    "eslint": "^8.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.6.2",
    "vitest": "^1.0.0"
  }
}
